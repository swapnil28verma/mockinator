{
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"name": "mock-builder",
	"projectType": "library",
	"sourceRoot": "projects/mock-builder/src",
	"prefix": "lib",
	"targets": {
		"build": {
			"executor": "@angular-devkit/build-angular:ng-packagr",
			"options": {
				"project": "projects/mock-builder/ng-package.json"
			},
			"configurations": {
				"production": {
					"tsConfig": "projects/mock-builder/tsconfig.lib.prod.json"
				},
				"development": {
					"tsConfig": "projects/mock-builder/tsconfig.lib.json"
				}
			},
			"defaultConfiguration": "production",
			"outputs": ["{workspaceRoot}/dist/mock-builder"]
		},
		"test": {
			"executor": "@angular-devkit/build-angular:karma",
			"options": {
				"main": "projects/mock-builder/src/test.ts",
				"tsConfig": "projects/mock-builder/tsconfig.spec.json",
				"karmaConfig": "projects/mock-builder/karma.conf.js",
				"polyfills": ["zone.js"],
				"sourceMap": true,
				"styles": [],
				"scripts": [],
				"assets": []
			}
		},
		"lint": {
			"executor": "@nx/linter:eslint",
			"options": {
				"lintFilePatterns": ["projects/mock-builder/**/*.ts"]
			}
		}
	}
}
